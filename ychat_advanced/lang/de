# Chat-help ( || separiert Befehlssyntax und Befehlsbeschreibung. )
mods/commands/yc_away.so="/away AWAYNACHRICHT||Sich vom Chat kurzfristig abmelden";
mods/commands/yc_col.so="/col NICKFARBE TEXTFARBE||Ändern der Farbe";
mods/commands/yc_getrusage.so="/getrusage||Zeigt informationen zum laufdenen ychat prozess an";
mods/commands/yc_m.so="/m AKTIONSTEXT||Schreibt den Text kursiv ( zweifarbig )";
mods/commands/yc_me.so="/me AKTIONSTEXT||Schreibt den Text kursiv ( einfarbig )";
mods/commands/yc_msg.so="/msg NICKNAME TEXT||Flüstert TEXT zu NICKNAME";
mods/commands/yc_version.so="/version||Zeigt die Version des laufenden yChats";
mods/commands/yc_q.so="/q||Verlassen des Chats";
mods/commands/yc_time.so="/time||Zeigt die Serversystemzeit an";
mods/commands/yc_uptime.so="/uptime||Zeigt die yChat Uptime an";
mods/commands/yc_s.so="/s TEXT||Schreit den TEXT";
mods/commands/yc_topic.so="/topic TOPIC||Ändert das Thema des Raumes nach TOPIC";
